draw_text(view_wview[1] / 2, view_hview[1] - 60, string(hp) + "/" + string(maxhp));
draw_text(view_wview[1] / 2, view_hview[1] - 25, string(mana) + "/" + string(maxmana));

// Draw spells
size = 64;
top = alarm[2] / (room_speed * 5) * size;
draw_set_halign(fa_top)
draw_sprite_part(spr_spell_dash,1,0,0,size,size,view_wview[1] / 2 - view_wview[1] / 4, view_hview[1]-80-size);
draw_sprite_part(spr_spell_dash,0,0,top,size,size-top,view_wview[1] / 2 - view_wview[1] / 4, view_hview[1]-80-size+top);



// Draw entetities vagy mi a kuki :D
