        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Get input and move
var hspd = spd * (keyboard_check(ord('D')) - keyboard_check(ord('A')));
var vspd = spd * (keyboard_check(ord('S')) - keyboard_check(ord('W')));

// Move
scr_move(hspd, vspd);

// Set depth
depth = -y;

// Set direction
if(keyboard_check(ord('W'))){dirc = 1}
if(keyboard_check(ord('S'))){dirc = 3}
if(keyboard_check(ord('A'))){dirc = 2}
if(keyboard_check(ord('D'))){dirc = 0}

// Manareg
if(canManareg and mana &lt; maxmana){
    mana++;
}

// Dash
if(keyboard_check(ord('2')) and canDash and mana &gt;= 50){
    spd = 6;
    dashing = true;
    mana -= 50;
    canDash = false;
    canManareg = false;
    alarm[1] = room_speed/2;
    alarm[2] = room_speed * 5;
    alarm[10] = room_speed * 3;
}
if(dashing){
    instance_create(x,y,obj_player_dash);
}

// Player death
if(hp &lt;= 0){
    show_message("Game Over");
   room_restart();
}

</string>
